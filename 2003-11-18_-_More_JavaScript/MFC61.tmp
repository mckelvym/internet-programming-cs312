<html>
	<head>
	    <script language="JavaScript">
	    var theTitle = "";
	    function changeTitle(theTitle){
  			document.write("<title> " + theTitle + "</title>");
	    }
		</script>
	</head>
	<body>
	<script language="JavaScript">
	  theTitle = prompt("What do you want the title to be?","");
		changeTitle(theTitle);
		name = prompt("What is your name?","");
		alert("Greetings " + name + "!");
		site = prompt("Enter the website you want to visit","");
		switch(site){
  		case "msn":
  			window.location = "http://www.msn.com";
  		case "download":
  			window.location = "http://www.download.com";
  		case "tweakxp":
  			window.location = "http://www.tweakxp.com";
  		default:
  			window.location = "http://www.google.com";
		}
	</script>
	</body>
</html>
	
	
	
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		  
	 		
	 	
	 	
	 	
	 	
	 	
	 	
	 	
	 	
	 	
	 	
	 	
	 	
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 
	
	
		